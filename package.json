{
  "name": "natural-language-trading-tu",
  "version": "1.0.0",
  "description": "A natural language trading CLI application for executing stock trades through conversational commands",
  "main": "dist/index.js",
  "bin": {
    "nltrade": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
